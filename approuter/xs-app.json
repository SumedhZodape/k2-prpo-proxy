{
  "welcomeFile": "/health", 
  "authenticationMethod": "route",
  "routes": [
    {
      "source": "^/health", 
      "target": "/health",
      "destination": "srv-api",
      "authenticationType": "none"
    },
    {
      "source": "^/odata/v4/(.*)$",
      "target": "/odata/v4/$1",
      "destination": "srv-api",
      "authenticationType": "xsuaa"
    },
    {
      "source": "^/(.*)$", 
      "target": "$1",
      "destination": "srv-api",
      "authenticationType": "xsuaa"
    }
  ]
}